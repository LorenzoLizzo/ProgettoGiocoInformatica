<Window x:Class="ProgettoGiocoInformatica.ImpostazioniWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProgettoGiocoInformatica"
        mc:Ignorable="d"
        Title="Impostazioni" Height="450" Width="800">
    <Grid>
        <CheckBox x:Name="ckXfx" Content="Effetti sonori" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Checked="ckXfx_Checked"/>
        <CheckBox x:Name="ckIntero" Content="Schermo Intero" HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" Checked="ckIntero_Checked"/>
        <CheckBox x:Name="ckMusica" Content="Musica" HorizontalAlignment="Left" Margin="10,25,0,0" VerticalAlignment="Top" Checked="ckMusica_Checked"/>
        <Button x:Name="btnIndietro" Content="Indietro" HorizontalAlignment="Left" Margin="10,78,0,0" VerticalAlignment="Top" Width="75" Height="48" Click="btnIndietro_Click"/>
    </Grid>
</Window>
